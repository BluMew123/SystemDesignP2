generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model TarotEntry {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  date       DateTime @db.Date
  spread     String
  spreadValue String?
  notes      String?
  cards      Card[]
  createdAt  DateTime @default(now())
}

type Card {
  name     String
  position String
}
